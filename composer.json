{
    "name": "codaptive/laravel-pinpoint",
    "description": "AWS Pinpoint driver for Laravel",
    "homepage": "https://github.com/codaptive/laravel-pinpoint",
    "require": {
        "php": ">=7.3",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0",
        "aws/aws-sdk-php": "^3.133"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Banks",
            "email": "richard.banks@codaptive.io"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Codaptive\\LaravelPinpoint\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codaptive\\LaravelPinpoint\\PinpointServiceProvider"
            ]
        }
    }
}
